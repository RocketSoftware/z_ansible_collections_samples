//DAJ@BULD EXEC PGM=IKJEFT1A,REGION=1024M                               
//*                                                           
//DAJERROR DD SYSOUT=*                                        
//DLC$EXCP DD SYSOUT=*                                        
//EXCEPTNS DD SYSOUT=*                                        
//RUNSTATS DD SYSOUT=*                                        
//TRIGGERS DD SYSOUT=*                                        
//UTPRINT  DD SYSOUT=*                                        
//STPRIN01 DD SYSOUT=*                                        
//SYSTSPRT DD SYSOUT=*                                        
//SYSOUT   DD SYSOUT=*                                        
//DLCDEBUG DD SYSOUT=*                                        
//DB2PARMS DD DSN=RSTEST.DAJ110.CONTROL,DISP=SHR              
//SYSPROC  DD DSN=RSTEST.DAJ110.CLIST,DISP=SHR                
//STEPLIB  DD DSN=RSDEMO.DAJ110.IBMTAPE.SDAJLOAD,DISP=SHR     
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SFECLOAD,DISP=SHR     
//         DD DSN={{ daj_sdsnexit }},DISP=SHR                            
//         DD DSN={{ daj_sdsnload }},DISP=SHR                            
//ISPLLIB  DD DSN=RSDEMO.DAJ110.IBMTAPE.SDAJLOAD,DISP=SHR     
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SFECLOAD,DISP=SHR     
//         DD DSN={{ daj_sdsnexit }},DISP=SHR                            
//         DD DSN={{ daj_sdsnload }},DISP=SHR                            
//ISPPLIB  DD DSN=RSDEMO.DAJ110.IBMTAPE.SDAJPENU,DISP=SHR     
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SFECPENU,DISP=SHR     
//ISPTLIB  DD DSN=&&TEMP,DISP=(OLD,DELETE,DELETE)             
//         DD DSN=ISP.SISPTENU,DISP=SHR                       
//ISPMLIB  DD DSN=ISP.SISPMENU,DISP=SHR                       
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SDAJMENU,DISP=SHR     
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SFECMENU,DISP=SHR     
//ISPSLIB  DD DSN=RSTEST.DAJ110.IBMTAPE.SDAJSLIB.USER,DISP=SHR
//         DD DSN=RSDEMO.DAJ110.IBMTAPE.SDAJSLIB,DISP=SHR     
//ISPPROF  DD DSN=&&PROF,DISP=(NEW,DELETE),                   
//            UNIT=SYSALLDA,SPACE=(TRK,(2,1,2)),              
//            DCB=(RECFM=FB,LRECL=80,BLKSIZE=800)             
//ISPFILE  DD DSN=TS5941.DAJ.BUILD,DISP=SHR                   
//ISPLOG   DD SYSOUT=*,DCB=(RECFM=VA,LRECL=125,BLKSIZE=129)   
//ISPWRK1  DD UNIT=SYSALLDA,SPACE=(CYL,(30,30)),              
//            DCB=(RECFM=FB,LRECL=133,BLKSIZE=1330)           
//SYSTSIN  DD *                                               
  PROFILE  NOPREFIX                                           
  ISPSTART PGM(DAJ@BULD)                                      
//DAJ#DATA DD *                                                       
  GENERATE_UTILITY_JOB (                                             -
    DB2_SUBSYSTEM      {{ daj_ssid }} -
    USER_INDICATOR     DAJ                                           -
    DB2_SQLID          {{ ansible_user | upper }} -
    PROFILE_NAME       '{{ daj_profile_name }}' - 
    PROFILE_CREATOR    {{ daj_profile_creator }} - 
    PROFILE_DESCRIPTION ' '                                          -
    EXECUTION_LIB_2    RSDEMO.DAJ110.IBMTAPE.SDAJLOAD                -
    EXECUTION_LIB_4    RSDEMO.DAJ110.IBMTAPE.SFECLOAD                -
    DEBUG_MODE         OFF                                           -
    PREVIEW_EXCP_REPORT YES                                          -
    GEN_TO_DATASET     {{ ansible_user | upper }}.DAJ.BUILD -
    GEN_TO_MEMBER      REORG                                         -
                       )                                              
